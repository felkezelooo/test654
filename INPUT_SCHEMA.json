{
    "title": "YouTube & Rumble View Bot (Minimal Schema Test)",
    "description": "Test schema for objectList.",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "videosToWatch": {
            "title": "Videos to Watch",
            "type": "array",
            "description": "List of video URLs to process.",
            "editor": "objectList",
            "items": {
                "type": "object",
                "properties": {
                    "url": {
                        "title": "Video URL",
                        "type": "string",
                        "description": "YouTube or Rumble video URL",
                        "editor": "textfield"
                    }
                },
                "required": ["url"]
            },
            "default": [
                { "url": "https://www.youtube.com/watch?v=dQw4w9WgXcQ" }
            ]
        },
        "watchTimePercentage": {
            "title": "Watch Time Percentage",
            "type": "integer",
            "default": 80
        },
        "headless": {
            "title": "Headless Mode",
            "type": "boolean",
            "default": true
        },
        "concurrency": {
            "title": "Concurrency",
            "type": "integer",
            "default": 1
        }
    },
    "required": ["videosToWatch"]
}
